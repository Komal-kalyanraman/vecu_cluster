{
    "name": "FreeRTOS Dev Container",
    "dockerFile": "Dockerfile",
    "workspaceFolder": "/workspace",
    "mounts": [
        "source=${localWorkspaceFolder}/src,target=/workspace/src,type=bind",
        "source=${localWorkspaceFolder}/tasks,target=/workspace/tasks,type=bind"
    ],
    "remoteUser": "vscode"
}